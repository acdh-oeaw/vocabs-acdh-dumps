            <skos:hasTopConcept>
              <skos:Concept rdf:about="https://vocabs.dariah.eu/bbt/Concept/000002">
                <skos:narrower>
                  <skos:Concept rdf:about="https://vocabs.dariah.eu/bbt/Concept/000015">
                    <skos:scopeNote xml:lang="en">This term classifies changes in states, things and entities that can be attributed to activity of natural phenomena and whose effect is such as to cause the dissolution or modification of the subject of these changes to the degree that their identity is destroyed.  </skos:scopeNote>
                    <skos:prefLabel xml:lang="en">natural disasters</skos:prefLabel>
                    <skos:broader rdf:resource="https://vocabs.dariah.eu/bbt/Concept/000002"/>
                    <skos:inScheme rdf:resource="https://vocabs.dariah.eu/bbt/ConceptScheme/Backbone_Thesaurus"/>
                    <dct:creator>BBT maintenance WG</dct:creator>
                    <dct:modified>2016-02-09T09:16:59Z</dct:modified>
                    <dct:dateSubmitted>2015-08-13T07:18:40Z</dct:dateSubmitted>
                    <skos:notation>000015</skos:notation>
                  </skos:Concept>
                </skos:narrower>
                <skos:scopeNote xml:lang="en">This term classifies changes in states of affairs, things and entities that result from natural phenomena (e.g. earthquakes, floods).</skos:scopeNote>
                <skos:topConceptOf rdf:resource="https://vocabs.dariah.eu/bbt/ConceptScheme/Backbone_Thesaurus"/>
                <skos:prefLabel xml:lang="en">natural processes</skos:prefLabel>
                <skos:narrower rdf:resource="https://vocabs.dariah.eu/bbt/Concept/000016"/>
                <skos:inScheme rdf:resource="https://vocabs.dariah.eu/bbt/ConceptScheme/Backbone_Thesaurus"/>
                <dct:dateSubmitted>2015-08-13T07:33:06Z</dct:dateSubmitted>
                <skos:notation>000002</skos:notation>
                <dct:creator>BBT maintenance WG</dct:creator>
                <dct:modified>2016-02-09T08:44:59Z</dct:modified>
              </skos:Concept>
