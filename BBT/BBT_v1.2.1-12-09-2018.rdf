                    <skos:narrowMatch rdf:resource="http://83.212.173.34:8080/THEMAS-EL-DEMOS/HUMANITIES/Concept/232"/>
                    <skos:inScheme rdf:resource="https://vocabs.dariah.eu/bbt/ConceptScheme/Backbone_Thesaurus"/>
                    <skos:notation>000011</skos:notation>
                    <rdfs:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
                    <dct:creator>BBT maintenance WG</dct:creator>
                    <skos:prefLabel xml:lang="en">human interactions</skos:prefLabel>
                    <dct:modified>2016-02-09T09:16:59Z</dct:modified>
                    <skos:narrowMatch rdf:resource="http://83.212.173.34:8080/THEMAS-EL-DEMOS/HUMANITIES/Concept/643"/>
                    <skos:note xml:lang="en">Narrower term examples: "social events, confrontations or conflicts", "political", "social and economic occurrences", "group management"</skos:note>
                    <skos:scopeNote xml:lang="en">This term classifies intentional activities carried out by at least one actor causing or changing phenomena or states of affairs on the social, political, financial, cultural and intellectual level.</skos:scopeNote>
                    <skos:broader rdf:resource="https://vocabs.dariah.eu/bbt/Concept/000001"/>
                  </skos:Concept>
                </skos:narrower>
              </skos:Concept>
