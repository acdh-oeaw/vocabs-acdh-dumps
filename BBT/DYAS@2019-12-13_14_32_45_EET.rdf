    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
    <skos:prefLabel xml:lang="en">Accounts (Public inscriptions)</skos:prefLabel>
    <iso25964:status>Εκδοθείς</iso25964:status>
    <dcterms:source>Ζαββού 2007</dcterms:source>
    <skos:scopeNote xml:lang="en">Inscriptions that refer to ministrations concerning the property of a city-state or a sanctuary, such as safekeeping and managing it.</skos:scopeNote>
    <skos:narrower rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/1258"/>
