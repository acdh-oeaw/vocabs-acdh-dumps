  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/120">
    <skos:altLabel xml:lang="el">Τρόποι (μουσική θεωρία)</skos:altLabel>
    <dcterms:contributor xml:lang="en">Terzis Christos (AA)</dcterms:contributor>
    <skos:prefLabel xml:lang="el">Αρμονίες (αρμονική επιστήμη)</skos:prefLabel>
    <dcterms:created>2015-04-17</dcterms:created>
    <iso25964:status>Εκδοθείς</iso25964:status>
    <skos:scopeNote xml:lang="el">Είναι οι επτά διαφορετικές διατάξεις κατά τη συνεχή (ανιούσα ή κατιούσα) διαδοχή των φθόγγων μέσα στο οκτάχορδο σύστημα (σύστημα δια πασών).</skos:scopeNote>
    <skos:altLabel xml:lang="el">Είδη του δια πασών</skos:altLabel>
    <dcterms:contributor xml:lang="el">Τερζής Χρήστος (ΑΑ)</dcterms:contributor>
    <dcterms:creator xml:lang="el">Τερζής Χρήστος (ΑΑ)</dcterms:creator>
    <skos:related rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/427"/>
