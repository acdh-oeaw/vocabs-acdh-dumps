    <skos:prefLabel xml:lang="el">Σφραγίδες (Νομισματική)</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/644">
    <skos:related rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/296"/>
    <skos:altLabel xml:lang="el">Χρωματικόν</skos:altLabel>
