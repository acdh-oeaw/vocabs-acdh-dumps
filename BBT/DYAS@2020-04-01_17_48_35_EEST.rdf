  </rdf:Description>
  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/1334">
    <skos:notation>1334</skos:notation>
    <dcterms:created>2015-09-02</dcterms:created>
    <skos:prefLabel xml:lang="en">Narratology</skos:prefLabel>
    <dcterms:contributor xml:lang="en">Vernardaki Eleni (AA)</dcterms:contributor>
    <skos:scopeNote xml:lang="en">A branch of the Literary Theory, whose object is the study of the narrative techniques used by the narrative discourse in order to unroll a story.</skos:scopeNote>
    <skos:prefLabel xml:lang="el">Αφηγηματολογία (Τομείς απασχόλησης)</skos:prefLabel>
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
    <skos:topConceptOf rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
    <dcterms:creator xml:lang="en">Kalafata Patritsia (AA)</dcterms:creator>
