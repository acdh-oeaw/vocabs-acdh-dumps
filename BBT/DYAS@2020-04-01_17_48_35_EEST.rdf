    <dcterms:source>Τουράτσογλου (et al.) 2002</dcterms:source>
    <skos:scopeNote xml:lang="en">Wooden tablets coated with lime, plaster or wax and used in ancient times for informal announcements of the State, statements by various authorities and schoolwork.</skos:scopeNote>
    <skos:narrower rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/818"/>
    <dcterms:created>2015-07-02</dcterms:created>
    <dcterms:modified>2018-12-03</dcterms:modified>
    <skos:prefLabel xml:lang="en">Albums (Bearers of inscriptions)</skos:prefLabel>
    <dcterms:creator xml:lang="en">Chrysovitsanos Gerasimos (AA)</dcterms:creator>
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
