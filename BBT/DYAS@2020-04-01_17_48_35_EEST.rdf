    <skos:scopeNote xml:lang="en">The business or practice of regularly producing and disseminating information about affairs of public interest and importance.</skos:scopeNote>
    <dcterms:modified>2019-08-20</dcterms:modified>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
