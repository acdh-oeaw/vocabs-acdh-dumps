    <dcterms:modified>2015-10-06</dcterms:modified>
    <skos:notation>120</skos:notation>
    <skos:prefLabel xml:lang="en">Harmoniai</skos:prefLabel>
    <skos:related rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/713"/>
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
    <dcterms:creator xml:lang="en">Terzis Christos (AA)</dcterms:creator>
    <skos:related rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/45"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:broader rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/25"/>
    <skos:related rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/716"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/354">
    <dcterms:source>Pavis 2006</dcterms:source>
    <skos:broader rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/24"/>
    <skos:scopeNote xml:lang="en">Specially designed objects used for covering the face or parts of it, resulting in changing its appearance or making it unrecognizable. In theatre, the use of masks denaturalises the character by introducing a foreign body into the process of identification between spectator and actor. It is therefore often used when the stage direction seeks to avoid emotional transference and defamiliarises the character.</skos:scopeNote>
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
