  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/1234">
    <skos:prefLabel xml:lang="el">Νευρογλωσσολογία</skos:prefLabel>
    <dcterms:source>Ahlsén 2006</dcterms:source>
    <dcterms:modified>2015-11-02</dcterms:modified>
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
    <dcterms:creator xml:lang="en">Karasimos Athanasios (AA)</dcterms:creator>
