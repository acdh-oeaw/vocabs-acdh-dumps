    <skos:narrower rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/1822"/>
    <skos:broader rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/34"/>
