    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dcterms:modified>2019-09-23</dcterms:modified>
    <skos:related rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/1641"/>
    <dcterms:source>Allaby 2013</dcterms:source>
    <skos:notation>1623</skos:notation>
    <skos:scopeNote xml:lang="en">Subdiscipline of Geology that studies rocks, including the processes of their production, their structure, mineral composition and classification, as well as their origin and history.</skos:scopeNote>
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
