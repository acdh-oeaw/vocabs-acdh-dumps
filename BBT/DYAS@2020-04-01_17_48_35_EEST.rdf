  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/1613">
    <dcterms:contributor xml:lang="el">Κατσιαδάκη Ελένη (ΑΑ)</dcterms:contributor>
    <skos:prefLabel xml:lang="el">Γενεαλογία</skos:prefLabel>
