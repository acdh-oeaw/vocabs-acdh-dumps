  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/1010">
    <dcterms:creator xml:lang="en">Xiropaidis Georgios (ASFA)</dcterms:creator>
    <dcterms:contributor xml:lang="el">Ξηροπαϊδης Γιώργος (ΑΣΚΤ)</dcterms:contributor>
    <dcterms:source>The J. Paul Getty Trust 2015</dcterms:source>
    <dcterms:created>2015-10-05</dcterms:created>
    <dcterms:creator xml:lang="el">Ξηροπαϊδης Γιώργος (ΑΣΚΤ)</dcterms:creator>
    <skos:altLabel xml:lang="el">Μικτή τεχνική</skos:altLabel>
    <skos:broader rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/378"/>
    <iso25964:status>Εκδοθείς</iso25964:status>
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
    <skos:scopeNote xml:lang="en">Any combination of a variety of materials plus the associated techniques, used in the making of a single work of art. In Printmaking, use when more than one technique, such as both etching and engraving, are used in one print. For the painting technique of laying oil paint glazes over tempera paint or tempera paint glazes over oil paint, use "Mixed technique".</skos:scopeNote>
    <dcterms:modified>2015-10-30</dcterms:modified>
    <skos:prefLabel xml:lang="el">Μικτά μέσα</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Mixed media</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:scopeNote xml:lang="el">Συνδυασμός μέσων, υλικών και τεχνικών για την παραγωγή ενός έργου τέχνης.</skos:scopeNote>
    <skos:notation>1010</skos:notation>
    <dcterms:contributor xml:lang="en">Xiropaidis Georgios (ASFA)</dcterms:contributor>
  </rdf:Description>
  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/669">
