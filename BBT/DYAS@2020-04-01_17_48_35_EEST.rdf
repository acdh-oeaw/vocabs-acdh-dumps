    <skos:altLabel xml:lang="el">Ιάσεις</skos:altLabel>
    <skos:prefLabel xml:lang="en">Miracle inscriptions</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
