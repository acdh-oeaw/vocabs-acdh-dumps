    <dcterms:modified>2018-12-11</dcterms:modified>
    <skos:prefLabel xml:lang="el">Φωτογραφίες επιγραφών</skos:prefLabel>
    <dcterms:contributor xml:lang="en">Katsiadakis Helen (AA)</dcterms:contributor>
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
    <dcterms:source>Woodhead 2009</dcterms:source>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/881">
    <dcterms:modified>2015-10-04</dcterms:modified>
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
    <dcterms:creator xml:lang="el">Χατζημιχαήλ Χρήστος (ΑΑ)</dcterms:creator>
    <dcterms:created>2015-10-03</dcterms:created>
    <skos:prefLabel xml:lang="en">Interjection</skos:prefLabel>
    <iso25964:status>Εκδοθείς</iso25964:status>
    <skos:scopeNote xml:lang="el">Επιφωνήματα λέγονται οι άκλιτες λέξεις που φανερώνουν ψυχικό πάθημα, όπως θαυμασμό, ενθουσιασμό, χαρά ή αγανάκτηση, αποστροφή, λύπη κτλ.</skos:scopeNote>
    <skos:prefLabel xml:lang="el">Επιφώνημα</skos:prefLabel>
    <dcterms:source>Οικονόμου 1999</dcterms:source>
    <dcterms:contributor xml:lang="en">Katsiadakis Helen (AA)</dcterms:contributor>
    <skos:topConceptOf rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
    <skos:notation>881</skos:notation>
