    <iso25964:status>Εκδοθείς</iso25964:status>
    <skos:prefLabel xml:lang="en">Nuclear warfare</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/1498">
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
