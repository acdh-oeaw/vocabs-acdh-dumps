    <skos:prefLabel xml:lang="el">Γραφή παρανόμων</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dcterms:modified>2019-10-29</dcterms:modified>
