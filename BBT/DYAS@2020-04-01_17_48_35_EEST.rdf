    <skos:prefLabel xml:lang="fr">Prefixe par dérivation</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/1702">
