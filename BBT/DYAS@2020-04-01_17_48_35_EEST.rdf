    <dcterms:modified>2015-10-30</dcterms:modified>
    <skos:scopeNote xml:lang="el">Έργα τέχνης φιλοτεχνημένα με χρωστική ύλη που αναμιγνύεται με νερό και αποτυπώνεται με χρωστήρα.</skos:scopeNote>
    <skos:prefLabel xml:lang="el">Υδατογραφίες</skos:prefLabel>
    <skos:prefLabel xml:lang="en">Aquarelles</skos:prefLabel>
    <dcterms:source>The J. Paul Getty Trust 2015</dcterms:source>
  </rdf:Description>
  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/223">
