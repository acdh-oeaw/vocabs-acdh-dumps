    <skos:narrower rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/2063"/>
    <skos:narrower rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/2057"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:narrower rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/2049"/>
    <dcterms:creator xml:lang="en">Souyioultzoglou Iraklitos (AA)</dcterms:creator>
    <skos:prefLabel xml:lang="el">Γεωργία</skos:prefLabel>
    <skos:scopeNote xml:lang="el">Η συστηματική καλλιέργεια του εδάφους, η παραγωγή και η πρωτογενής επεξεργασία φυτικών προϊόντων, με κύριο σκοπό την κάλυψη αναγκών διατροφής και ένδυσης.</skos:scopeNote>
    <dcterms:source>Χαραλαμπάκης 2014</dcterms:source>
    <skos:narrower rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/2056"/>
    <skos:inScheme rdf:resource="https://humanitiesthesaurus.academyofathens.gr"/>
    <dcterms:modified>2019-09-26</dcterms:modified>
    <skos:prefLabel xml:lang="en">Horticulture</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/713">
    <skos:related rdf:resource="https://humanitiesthesaurus.academyofathens.gr/dyas-resource/Concept/428"/>
    <skos:notation>713</skos:notation>
