  a skos:ConceptScheme ;
  dc11:title "ARCHE Resource Type Category"@en, "Kategorien für Ressourcen in ARCHE"@de ;
  rdfs:label "Resource Type Category"@en, "Kategorien für Ressourcen"@de ;
  dc11:description "The general resource type an object can have based on controlled vocabulary of DataCite metadata schema 4.4."@en, "Der generelle Ressourcentyp für ein Objekt. Basiert auf dem kontrollierten Vokabular des Metadatenschemas von DataCite 4.4."@de ;
  dct:modified "2022-08-08";
  dc11:creator "Martina Trognitz" ;
