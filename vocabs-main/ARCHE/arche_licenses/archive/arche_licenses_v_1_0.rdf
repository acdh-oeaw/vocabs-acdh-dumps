  </rdf:Description>
  <rdf:Description rdf:about="https://vocabs.acdh.oeaw.ac.at/archelicenses/noc-oklr">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2020-02-29T16:19:27.863841+00:00</dct:modified>
