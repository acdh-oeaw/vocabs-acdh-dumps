  <skos:Concept rdf:about="https://vocabs.acdh.oeaw.ac.at/archelicenses/noc-cr">
    <skos:prefLabel xml:lang="en">NoC-CR</skos:prefLabel>
    <skos:prefLabel xml:lang="de">Noc-CR</skos:prefLabel>
    <skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archelicenses/Schema"/>
