    <skos:prefLabel xml:lang="de">CC BY-ND 4.0</skos:prefLabel>
    <skos:inScheme rdf:resource="https://vocabs.acdh.oeaw.ac.at/archelicenses/Schema"/>
    <skos:altLabel xml:lang="de">Namensnennung - Keine Bearbeitungen 4.0 International (CC BY-ND 4.0)</skos:altLabel>
