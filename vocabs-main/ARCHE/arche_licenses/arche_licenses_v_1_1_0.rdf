  <skos:Concept rdf:about="https://vocabs.acdh.oeaw.ac.at/archelicenses/und">
    <skos:prefLabel xml:lang="en">UND</skos:prefLabel>
    <skos:prefLabel xml:lang="de">UND</skos:prefLabel>
