<https://vocabs.acdh.oeaw.ac.at/indigo/graffitiConcepts> a skos:Concept ;
    dc:creator "Jona Schlegel",
        "Stefan Wogrin" ;
    dct:created "2023-05-17T15:58:11.899038+00:00"^^xsd:dateTime ;
