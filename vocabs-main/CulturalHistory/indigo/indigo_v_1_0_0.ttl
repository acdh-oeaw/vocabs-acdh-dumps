    dct:created "2023-05-17T15:57:55.643671+00:00"^^xsd:dateTime ;
    dct:modified "2023-08-10T09:56:20.627832+00:00"^^xsd:dateTime ;
    skos:altLabel "halo (glories)"@en ;
    skos:broader <https://vocabs.acdh.oeaw.ac.at/indigo/glories> ;
    skos:editorialNote "This concept is taken from the Getty AAT."@en ;
    skos:exactMatch <http://vocab.getty.edu/aat/300375684>,
